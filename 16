#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    int i, j, len;
    char n[100];
    scanf("%s", &n);
    len = strlen(n);
    if (len>10)  
    {
        printf("out of range\n");
        exit(0);
}
for (i = 0; i <= len; i++){
    switch (n[i]) {
case '0':
break;
case '1':  
printf("壹");
break;
case '2':
printf("貳");
break;
case '3':
printf("參");
break;
case '4':
printf("肆");
break;
case '5':
printf("伍");
break;
case '6':
printf("陸");
break;
case '7':
printf("柒");
break;
case '8':
printf("捌");
break;
case '9':
printf("玖");

break;
}
}
 
    return 0;
}
